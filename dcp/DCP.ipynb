    "# DCP analysis in CVXPY\n",
    "from cvxpy import *\n",
    "x = Variable()\n",
    "y = Variable()\n",
    "expr = quad_over_lin(x - y, 1 - max_elemwise(x,y))\n",
    "print \"expression curvature =\", expr.curvature\n",
    "print \"expression sign =\", expr.sign\n",
    "print \"expression is DCP?\", expr.is_dcp()"
   ]
